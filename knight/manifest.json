{
    "_metadata": {
        "major_version": 1,
        "minor_version": 1
    },
    "display_information": {
        "name": "knight"
    },
    "features": {
        "app_home": {
            "home_tab_enabled": false,
            "messages_tab_enabled": false,
            "messages_tab_read_only_enabled": true
        },
        "bot_user": {
            "display_name": "knight",
            "always_online": true
        }
    },
    "oauth_config": {
        "scopes": {
            "bot": [
                "channels:history",
                "channels:read",
                "groups:read",
                "groups:write",
                "chat:write",
                "commands",
                "emoji:read",
                "files:read",
                "groups:history",
                "im:history",
                "mpim:history",
                "usergroups:read",
                "users:read",
                "app_mentions.read",
                "dnd:read"
            ]
        }
    },
    "settings": {
        "event_subscriptions": {
            "bot_events": [
                "app_mention",
                "message.channels",
                "message.groups",
                "message",
                "channel_archive",
                "channel_created",
                "channel_deleted",
                "channel_unarchive",
                "channel_rename",
                "emoji_changed",
                "file_created",
                "file_deleted",
                "file_public",
                "file_shared",
                "file_unshared",
                "subteam_created",
                "subteam_updated",
                "subteam_members_changed",
                "team_join",
                "user_change",
                "user_huddle_changed",
                "dnd_updated_user"
            ]
        },
        "interactivity": {
            "is_enabled": true
        },
        "org_deploy_enabled": true,
        "socket_mode_enabled": true,
        "token_rotation_enabled": false
    }
}
